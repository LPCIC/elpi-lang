{
    "name": "elpi-lang",
    "displayName": "Elpi lang",
    "publisher": "gares",
    "description": "Syntax highlighting for Elpi",
    "version": "0.0.1",
    "homepage": "https://github.com/LPCIC/elpi-lang",
    "repository": {
        "type": "git",
        "url": "https://github.com/LPCIC/elpi-lang"
    },
    "keywords": ["elpi","lambda Prolog"],
    "license": "MIT",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "elpi",
                "aliases": [
                    "Elpi",
                    "elpi"
                ],
                "extensions": [
                    ".elpi"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "elpi",
                "scopeName": "source.elpi",
                "path": "./syntaxes/elpi.tmLanguage.json"
            }
        ]
    }
}
